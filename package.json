{
  "name": "@dodopayments/ingestion-blueprints",
  "version": "0.5.0",
  "type": "module",
  "description": "Sophisticated Dodo Payments Ingestion Blueprints",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist"
  },
  "keywords": [
    "dodo-payments",
    "usage-based-billing",
    "llm",
    "token-tracking",
    "ingestion",
    "billing"
  ],
  "author": "Dodo Payments",
  "dependencies": {
    "dodopayments": "^2.1.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/node": "^24.5.2",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "eslint": "^9.36.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.45.0",
    "tslib": "^2.8.1",
    "typescript": "^5.8.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dodopayments/ingestion-blueprints"
  }
}
